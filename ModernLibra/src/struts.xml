<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
   
<struts>
<constant name="struts.devMode" value="true" />	

	<package name="default" extends="struts-default">
		
		<action name="addStudent"
			class="com.ms.modernlibra.web.action.AddStudentAction" method="execute">
			<result name="success">/dashboard.jsp</result>
			<result name="error">/add_student.jsp</result>
			<result name="input">/add_student.jsp</result>
			<result name="addStudent">/add_student.jsp</result>
			
		</action>
		
		<action name="addBranch"
			class="com.ms.modernlibra.web.action.AddBranchAction" method="execute">
			<result name="success">/dashboard.jsp</result>
			<result name="error">/add_branch.jsp</result>
			<result name="input">/add_branch.jsp</result>
			<result name="addBranch">/add_branch.jsp</result>
		</action>

	</package>
</struts>
